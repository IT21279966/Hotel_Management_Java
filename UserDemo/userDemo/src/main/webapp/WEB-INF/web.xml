<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    
    <!-- For Log-In Page -->
     
    <servlet>    
		<servlet-name>Login</servlet-name>
		<servlet-class>userPack.loginServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Log</url-pattern>
	</servlet-mapping>
	
	
	<!-- For Help&Support -->
	
	<servlet>    
		<servlet-name>HelpSupport</servlet-name>
		<servlet-class>userPack.help_Sup_Insert</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>HelpSupport</servlet-name>
		<url-pattern>/helpsup</url-pattern>
	</servlet-mapping>
	
	
	<!-- ok button for Help&Support -->
	<servlet>    
		<servlet-name>OkSuccessHelpSupport</servlet-name>
		<servlet-class>userPack.helpSuccessServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>OkSuccessHelpSupport</servlet-name>
		<url-pattern>/okhelpsup</url-pattern>
	</servlet-mapping>
	 
	
	
	<!-- Update Help Support -->
	<servlet>    
		<servlet-name>updateHelpSupport</servlet-name>
		<servlet-class>userPack.updateHelp_Support_Servlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>updateHelpSupport</servlet-name>
		<url-pattern>/updatenow</url-pattern>
	</servlet-mapping>
	
	
	<!-- Delete Help Support -->
	<servlet>    
		<servlet-name>deleteHelpSupport</servlet-name>
		<servlet-class>userPack.deleteHelp_Support_Servlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>deleteHelpSupport</servlet-name>
		<url-pattern>/deletenow</url-pattern>
	</servlet-mapping>
	
</web-app>